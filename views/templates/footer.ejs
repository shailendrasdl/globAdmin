			/* Search email */
			function searchemail()
			{
				var email = $('#emailfind').val();
				if(email)
				{
					$.ajax({
						url: '/searchemail/' + email,
						method: 'POST',
						data: {id: email}			
					}).done(function(res) {	
						if(res)
