<!-- Header -->
<% include templates/header.ejs %>
<!-- Sidebar -->
<% include templates/sidebar.ejs %>
<!-- Automobile Body -->
<div class="container-fluid">
    <!-- Breadcrumbs-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <i class="fa fa-home"></i>
            <a href="/dashboard">Dashboard</a>
        </li>
        <li class="breadcrumb-item active">Automobile</li>
    </ol>
    <h3 class="box-title">Add Automobile details</h3>
    <form action="/post_automobile" method="post" enctype="multipart/form-data">
        <div class="row clearfix">
            <div class="col-md-9">
                <div class="pro-section">
                    <h6 class="user-title">Content</h6>
                    <div class="user-body">
                        <div class="row clearfix">
                            <div class="col-md-6">
                                    <!-- <label><h6>Category</h6></label> -->
                                    <input type="text" name="category" id="category"  value="automobile" class="form-control" hidden>
                                <div class="form-group">
                                    <label><h6>Name</h6></label>
                                    <input type="text" name="name" id="name" required class="form-control">
                                </div>
                                <div class="form-group">
                                    <label><h6>Keywords</h6></label>
                                    <input type="text" name="keywords" id="keywords" required class="form-control">
                                </div>
                                <div class="form-group">
                                    <label><h6>Address</h6></label>
                                    <textarea class="form-control" rows="4" name="address" required></textarea>
                                </div>
                            </div>
                            
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label><h6>Email</h6></label>
                                    <input type="text" name="email" id="email" required class="form-control">
                                </div>
                                <div class="form-group">
                                    <label><h6>Mobile</h6></label>
                                    <input type="text" name="mobile" id="mobile" maxlength="10" required class="form-control">
                                </div>
                                <div class="form-group">
                                    <label><h6>Description</h6></label>
                                    <textarea class="form-control" rows="4" name="description" required ></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pro-section">
                    <h6 class="user-title">Features</h6>
                    <div class="user-body">
                        <div class="row clearfix">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Make</h6></label>
                                    <input type="text" name="make" id="make" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Model</h6></label>
                                    <input type="text" name="model" id="model" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Variant</h6></label>
                                    <input type="text" id="variant" name="variant" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Engine</h6></label>
                                    <input type="text" id="engine" name="engine" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Horse Power</h6></label>
                                    <input type="text" id="horse_power" name="horse_power" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Mileage</h6></label>
                                    <input type="text" id="mileage" name="mileage" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Fuel</h6></label>
                                    <input type="text" id="fuel" name="fuel" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Emission class</h6></label>
                                    <input type="text" id="emission_class" name="emission_class" required class="form-control">
                                </div>
                            </div>
                    
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>1st Registration Year</h6></label>
                                    <input type="text" id="registration_year" name="registration_year" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Currency</h6></label>
                                    <input type="text" id="currency" name="currency" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Price</h6></label>
                                    <input type="text" id="price" name="price" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Warranty</h6></label>
                                    <input type="text" id="warranty" name="warranty" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Location</h6></label>
                                    <input type="text" id="location" name="location" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Color</h6></label>
                                    <input type="text" id="color" name="color" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Interior Seats</h6></label>
                                    <input type="text" id="interior_seats" name="interior_seats" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label><h6>Doors</h6></label>
                                    <input type="text" id="doors" name="doors" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label><h6>Seats</h6></label>
                                    <input type="text" id="seats" name="seats" required class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label><h6>Equipment Description</h6></label>
                                    <textarea class="form-control" id="equipment_description" required name="equipment_description" rows="4"></textarea>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label><h6>Accessories</h6></label>
                                    <textarea class="form-control" id="accessories" name="accessories" rows="4" required></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-3">
                <div class="pro-section">
                    <h6 class="user-title">Display Contact Details</h6>
                    <div class="user-body">
                        <input type="checkbox" name="display" value="mobile">&nbsp;&nbsp;<label>Mobile Number</label><br>
                        <input type="checkbox" name="display" value="email">&nbsp;&nbsp;<label>Email ID</label><br>
                        <input type="checkbox" name="display" value="address">&nbsp;&nbsp;<label>Address</label><br>
                        <input type="checkbox" name="display" value="location">&nbsp;&nbsp;<label>Location</label><br>
                    </div>
                </div>
                <div class="pro-section">
                    <h6 class="user-title">Image</h6>
                    <div class="user-body">
                        <div class=" upload_section">
                            <img src="/images/up_image.jpg" class="img-fluid">
                            <div class="upload_button">
                                <div class="vfile">
                                    <div class="text-center">
                                        <input type="file" name="video" id="video" required class="inputfile inputfile-1">
                                        <label for="video"><i class="fa fa-upload"></i>&nbsp;&nbsp;<span>Upload Video</span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
         
                <div class="pro-section">
                    <h6 class="user-title">Seller</h6>
                    <div class="user-body">
                        <div class="form-group">
                            <input type="radio" name="seller">&nbsp;&nbsp;Company<br>
                            <input type="radio" name="seller">&nbsp;&nbsp;Individual
                        </div>
                    </div>
                </div>
                <div class="pro-section">
                    <h6 class="user-title">Condition</h6>
                    <div class="user-body">
                        <div class="form-group">
                            <input type="radio" name="condition">&nbsp;&nbsp;USed<br>
                            <input type="radio" name="condition">&nbsp;&nbsp;New
                      </div>
                    </div>
                </div>
                <div class="pro-section">
                    <h6 class="user-title">Gear Type</h6>
                    <div class="user-body">
                        <div class="form-group">
                            <input type="radio" name="gear_type">&nbsp;&nbsp;Automatic<br>
                            <input type="radio" name="gear_type">&nbsp;&nbsp;Manual
                      </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row clearfix form-group ">
            <div class="col-md-2">
                <button class="btn btn-primary btn-sm w-100" type="submit"><i class="fas fa-fw fa-save"></i> Save</button>
            </div>
            <div class="col-md-2">
                <a href="/automobile" class="btn btn-warning btn-sm w-100"><i class="fas fa-fw fa-ban"></i> Cancel</a>
            </div>
        </div>
    </form>
</div>

<!-- Footer -->
<% include templates/footer.ejs %> 